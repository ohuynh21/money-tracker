{
    "version": 2,
    "builds": [
      {
        "src": "./api/index.js",
        "use": "@vercel/node"
      },
  
      
      {
        "src": "./client/package.json",
        "use": "@vercel/static-build"
      }
    
    ],
    "routes": [
      {
        "src": "api/.*",
        "dest": "api/index.js"
      },
  
 
      {
        "src": "client/build/static/.*",
        "dest": "static/.*"
      },
      {
        "src": "client/build/index.html",
        "dest": "index.html"
      }
  
    ]
}